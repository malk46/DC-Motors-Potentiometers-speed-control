

char reading;
void setup() {
  pinMode(11,INPUT);
  pinMode(12,INPUT);
  
  Serial.begin(9600);
  
}

void loop() {
if(Serial.available()){
 reading=Serial.read();
  if (reading=='L' || digitalWrite(12,HIGH){ 
    analogWrite(6 ,analogRead(A0));
}
      else{
        analogWrite(6,0);}
if(reading=='R' || digitalWrite(11,HIGH){
      analogWrite(10,analogRead(A2));
}
      else{
        analogWrite(10,0);}
                
